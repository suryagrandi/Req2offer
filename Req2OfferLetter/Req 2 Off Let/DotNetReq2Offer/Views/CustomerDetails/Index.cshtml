					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Customer__Name)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.SPOC)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Email_Id)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Contact_Number)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Location)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Address)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Creation_Date)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Flag)
					</td>
					<td>
						@Html.ActionLink("Edit", "Edit", new { id = item.Customer_Id }, htmlAttributes: new { @class = "btn btn-primary", @role = "button" })
					</td>
					<td>
						
							
							@Html.ActionLink("Delete", "Delete", new { id = item.Customer_Id }, new { onclick = "return confirm('Do you really want to delete" + item.Customer_Id + "?')", @class = "btn btn-primary", @role = "button" })
							
					</td>
				</tr>
			}

