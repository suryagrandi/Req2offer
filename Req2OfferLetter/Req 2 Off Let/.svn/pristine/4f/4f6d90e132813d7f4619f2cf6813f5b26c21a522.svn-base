				<script language="javascript" type="text/javascript">


		$(document).ready(function () {
			debugger;
			$("#AjaxPost").click(function () {
				//alert("Hi!!!!");
				$("#content").html("<b>Please Wait...</b>");

				var dataObject = {
					//Customer_Id: $("#Customer_Id").val(),
					Customer__Name: $("#Customer__Name").val(),
					SPOC: $("#SPOC").val(),
					Email_Id: $("#Email_Id").val(),
					Contact_Number: $("#Contact_Number").val(),
					Location: $("#Location").val(),
					Address: $("#Address").val(),
					Creation_Date: $("#Creation_Date").val(),
					Flag: $("#Flag").val()
				};
				$.ajax({
					url: "@Url.Action("Create", "CustomerDetails")",
					type: "POST",
					data: dataObject,
					dataType: "json",

					success: function (data) {
						//alert("Created");
						debugger;
						if (data.toString() == "Successfully Saved!") {
							$("#Customer_Id").val('');
							$("#Customer__Name").val('');
							$("#SPOC").val('');
							$("#Email_Id").val('');
							$("#Contact_Number").val('');
							$("#Location").val('');
							$("#Address").val('');			
							$("#Creation_Date").val('');
							$("#Flag").val('');

							$("#content").html("<div class='success'>" + data + "</div>");
							document.location.href = "Index";
						}
						else {
							$("#content").html("<div class='failed'>" + data + "</div>");
						}
					},
					error: function () {
						$("#content").html("<div class='failed'>Error! Please try again</div>");
					}
				});
			});
		});
				</script>

